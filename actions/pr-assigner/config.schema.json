{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "assignees": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "count": {
      "type": "integer",
      "minimum": 1
    },
    "shuffle": {
      "type": "integer",
      "minimum": 1
    }
  },
  "required": ["assignees"],
  "additionalProperties": false
}
